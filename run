#!/usr/bin/env python

from argparse import ArgumentParser

from cars.scrapers.edmunds import init_parser as init_edmunds
from cars.scrapers.truecar import init_parser as init_truecar


def main() -> None:

    parser = ArgumentParser(prog="cars")
    sub = parser.add_subparsers()

    init_truecar(sub.add_parser("scrape-truecar"))
    init_edmunds(sub.add_parser("scrape-edmunds"))

    args = parser.parse_args()
    args.exe(args)


if __name__ == "__main__":
    main()
